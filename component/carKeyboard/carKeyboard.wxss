.SevenDreamY_keyboard{
  background: #eee;
  padding: 30rpx 2rpx;
  width: 100%;
  position: fixed;
  bottom: 0;
}
.key_row{
  display: flex;
  justify-content: flex-start;
}
.special_row{
  display: flex;
  justify-content: center;
}
.com_key{
  margin: 6rpx;
  background: #f8f8f8;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  border-radius:6rpx; 
  align-items: center;
  box-shadow: 0rpx 2rpx 3rpx #333;
}
.key_item{
  width: 64rpx;
  height:80rpx;
}
.special_item {
    width: 80rpx;
    height:80rpx;
}
.ABC_key{
   width:140rpx;
   height:80rpx;
}
.Del{
  width:140rpx;
  height:80rpx;
  background: #ff5151;
  color: #fff;
}

.com_key:active{
  opacity: 0.6;
  box-shadow: 0rpx 2rpx 3rpx #666 inset;
}
.key_hover{
  font-weight: 650;
  color: #1989fa;
}
.prohibit_key{
  pointer-events: none;
  box-shadow: 0rpx 0rpx 0rpx;
  color: darkgray;
}